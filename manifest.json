{
  "manifest_version": 2,

  "name": "NJU GPA Calculator",
  "description": "使用浏览器登录教务处网站后，点击右上角的插件图标，即可计算GPA",
  "version": "1.0",

  "permissions": ["tabs", "notifications", "http://*/*", "https://*/*"], 
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://cdn.staticfile.org; object-src 'self'"
}